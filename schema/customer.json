{
  "$defs": {
    "Address": {
      "description": "Address model.",
      "properties": {
        "street": {
          "title": "Street",
          "type": "string"
        },
        "zip": {
          "title": "Zip",
          "type": "string"
        },
        "city": {
          "title": "City",
          "type": "string"
        },
        "country": {
          "anyOf": [
            {
              "pattern": "^(DE|Germany|Deutschland)$",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Country"
        }
      },
      "required": [
        "street",
        "zip",
        "city"
      ],
      "title": "Address",
      "type": "object"
    }
  },
  "description": "Customer model for the customer.yaml file.",
  "properties": {
    "customer_id": {
      "title": "Customer Id",
      "type": "integer"
    },
    "name": {
      "title": "Name",
      "type": "string"
    },
    "company": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Company"
    },
    "email": {
      "format": "email",
      "title": "Email",
      "type": "string"
    },
    "phone": {
      "format": "phone",
      "maxLength": 64,
      "minLength": 7,
      "title": "Phone",
      "type": "string"
    },
    "url": {
      "anyOf": [
        {
          "format": "uri",
          "maxLength": 2083,
          "minLength": 1,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Url"
    },
    "address": {
      "$ref": "#/$defs/Address"
    }
  },
  "required": [
    "customer_id",
    "name",
    "email",
    "phone",
    "address"
  ],
  "title": "Customer",
  "type": "object"
}